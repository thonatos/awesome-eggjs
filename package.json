{
  "name": "awesome-eggjs",
  "version": "0.1.0",
  "description": "Awesome Egg.js",
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --title=awesome-eggjs --env=prod",
    "startd": "egg-scripts start --daemon",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run eslint:fix && npm run tslint:fix && npm run test-local",
    "build": "npm run tsc && easywebpack build prod",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run tslint && npm run cov && npm run tsc",
    "autod": "autod",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "tslint": "tslint --project . -c tslint.json",
    "tslint:fix": "tslint --fix --project . -c tslint.json",
    "clean": "ets clean",
    "kill-port": "easy kill"
  },
  "dependencies": {
    "@octokit/rest": "^15.9.4",
    "egg-scripts": "^2.6.0",
    "egg-view-react-ssr": "^2.2.7",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "@types/node": "^10.5.1",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "antd": "^3.0.3",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "autoprefixer": "8.6.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "easywebpack-cli": "^3.8.0",
    "easywebpack-react": "^4.0.0",
    "egg-bin": "^4.7.1",
    "egg-webpack": "^4.0.0",
    "egg-webpack-react": "^2.0.0",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.1.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "ip": "^1.1.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "node-sass": "^4.6.0",
    "prettier": "^1.13.7",
    "sass-loader": "^6.0.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^4.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.2"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:thonatos/awesome-eggjs.git"
  },
  "author": "Suyi<thonatos.yang@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/thonatos/awesome-eggjs.git"
}
